{
  "name": "arbuga-front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "sitemap-build": "tsc -p tsconfig.background.json",
    "sitemap-run": "node dist_background/src/background/sitemap/sitemap.js",
    "sitemap": "run-s sitemap-build sitemap-run",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest --environment jsdom --root src/ --run --coverage",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen-background": "graphql-codegen --config codegen.background.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.5",
    "@quasar/extras": "^1.15.10",
    "@vue/apollo-composable": "^4.0.0-beta.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "quasar": "^2.11.5",
    "vue": "^3.2.45",
    "vue-deepunref": "^1.0.1",
    "vue-i18n": "9",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/client-preset": "2.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/typed-document-node": "^2.3.13",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@pinia/testing": "^0.0.15",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.2",
    "@quasar/vite-plugin": "^1.3.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.1",
    "@vitest/coverage-istanbul": "^0.29.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.15.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.0.12"
  }
}
